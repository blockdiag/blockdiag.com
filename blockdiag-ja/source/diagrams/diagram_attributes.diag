diagram {
  // ノードサイズの設定
  node_width = 200;  // デフォルト： 128
  node_height = 100;  // デフォルト： 40

  // ノード間隔の設定
  span_width = 240;  // デフォルト： 64
  span_height = 120;  // デフォルト： 40

  // フォントサイズの設定
  fontsize = 20;  // デフォルト： 11

  // ノード形状の設定
  default_shape = roundedbox;  // デフォルト： box

  A -> B [label = "Use long long\nedge label"];
  A -> C;
}
