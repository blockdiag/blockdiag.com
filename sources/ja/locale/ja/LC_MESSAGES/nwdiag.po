msgid "``align`` : \"left\", \"center\" or \"right\""
msgstr "``align`` : \"left\", \"center\" or \"right\""

# 0813c04b8df14f5ab6b9e9535c684961
#: ../../../sources/ja/nwdiag/sphinxcontrib.rst:111
