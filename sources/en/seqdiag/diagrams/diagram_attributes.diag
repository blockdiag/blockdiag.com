diagram {
  // Set edge metrix.
  edge_length = 300;  // default value is 192
  edge_height = 80;  // default value is 40

  // Set fontsize.
  fontsize = 16;  // default value is 11

  // Do not show activity line
  activation = none;

  // Numbering edges automaticaly
  autonumber = True;

  browser  -> webserver [label = "GET \n/index.html"];
  browser <-- webserver;
}
